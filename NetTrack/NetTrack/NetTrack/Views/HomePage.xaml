<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:fontawesome="clr-namespace:FontAwesome"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:viewModels="clr-namespace:NetTrack.ViewModels"
             x:DataType="viewModels:HomePageViewModel"
             x:Class="NetTrack.Views.HomePage"
>

    <ContentPage.BindingContext>
        <viewModels:HomePageViewModel />
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout Padding="10">
            <FlexLayout>
                <Label Text="{x:Static fontawesome:FontAwesomeIcons.Plus}" FontFamily="FAB" FontSize="Large" Margin="10,5,0,5" ></Label>
                <Label Text="{Binding user.fname}" ></Label>
            </FlexLayout>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>